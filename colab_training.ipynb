import pandas as pd
from sklearn.model_selection import train_test_split
from sklearn.ensemble import RandomForestClassifier
from sklearn.metrics import classification_report
import joblib

# Загрузка
df = pd.read_csv('/content/bi_cleaning_dataset.csv')
print(df.head())

# Выберем признаки (например, числовые) и метку
# Ниже пример — адаптируй под фактические названия колонок
df = df.dropna()  # удалим пропущенные значения
X = df.drop('target_column', axis=1)  # замени на реальное имя
y = df['target_column']

# Делим на train/test
X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2)

# Обучаем
model = RandomForestClassifier()
model.fit(X_train, y_train)

# Сохраняем модель
joblib.dump(model, 'model.pkl')

# Проверка
y_pred = model.predict(X_test)
print(classification_report(y_test, y_pred))
